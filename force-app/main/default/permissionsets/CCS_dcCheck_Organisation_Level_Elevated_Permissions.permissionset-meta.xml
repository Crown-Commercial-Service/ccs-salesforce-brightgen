    <recordTypeVisibilities>
        <recordType>Account.Organisation</recordType>
        <visible>true</visible>
    </recordTypeVisibilities>
    <recordTypeVisibilities>
        <recordType>Account.Supplier</recordType>
        <visible>true</visible>
    </recordTypeVisibilities>
