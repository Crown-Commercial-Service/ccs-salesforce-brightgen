<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Used for data loading - Allows updating of Audit Fields (eg Created Date) and allows assigning inactive users as owners</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>CCS Updating Audit Fields and Inactive users</label>
    <userPermissions>
        <enabled>true</enabled>
        <name>CreateAuditFields</name>
    </userPermissions>
    <userPermissions>
        <enabled>true</enabled>
        <name>PasswordNeverExpires</name>
    </userPermissions>
    <userPermissions>
        <enabled>true</enabled>
        <name>UpdateWithInactiveOwner</name>
    </userPermissions>
</PermissionSet>
