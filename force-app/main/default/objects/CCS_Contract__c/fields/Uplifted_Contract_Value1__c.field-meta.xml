<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Uplifted_Contract_Value1__c</fullName>
    <externalId>false</externalId>
    <formula>IF(  Count_of_Amendments__c   &lt;&gt; 0, Contract_Value__c +  Sum_of_Variation_Value__c, 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Provides the contract value plus the value of any amendments. The field is auto-populated based on the completed contract amendment related list records</inlineHelpText>
    <label>Uplifted Contract Value</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
