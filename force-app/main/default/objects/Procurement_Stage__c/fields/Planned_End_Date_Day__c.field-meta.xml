<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Planned_End_Date_Day__c</fullName>
    <externalId>false</externalId>
    <formula>CASE( 
MOD( Planned_End_Date__c - DATE( 1900, 1, 8 ), 7 ), 
0, &quot;Monday&quot;, 
1, &quot;Tuesday&quot;, 
2, &quot;Wednesday&quot;, 
3, &quot;Thursday&quot;, 
4, &quot;Friday&quot;, 
5, &quot;Saturday&quot;, 
6, &quot;Sunday&quot;, 
&quot;&quot; 
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>The day of the week of this stage&apos;s Planned End Date.</inlineHelpText>
    <label>Planned End Date Day</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
